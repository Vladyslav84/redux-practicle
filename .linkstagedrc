{
    *src/**/*.{json,css,scss,md}*: ["prettier --write"],
    *src/**/*.{js,jsx,ts,tsx}*: ["prettier --write", "eslint --fix"],
     "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always"

}